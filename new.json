{
    "abc_plus_commands": [
        {
            "syntax": "!staccato!",
            "category": "articulation",
            "function": "mark_note_short",
            "musical_intent": "Detached, light articulation",
            "musicxml": "<articulations><staccato/></articulations>"
        },
        {
            "syntax": "!tenuto!",
            "category": "articulation",
            "function": "mark_note_full_length",
            "musical_intent": "Sustained, emphasized note",
            "musicxml": "<articulations><tenuto/></articulations>"
        },
        {
            "syntax": "!accent!",
            "category": "articulation",
            "function": "add_accent",
            "musical_intent": "Strong attack emphasis",
            "musicxml": "<articulations><accent/></articulations>"
        },
        {
            "syntax": "!marcato!",
            "category": "articulation",
            "function": "add_strong_accent",
            "musical_intent": "Very forceful attack",
            "musicxml": "<articulations><strong-accent/></articulations>"
        },
        {
            "syntax": "!pizz!",
            "category": "technique",
            "function": "set_technique_pizzicato",
            "musical_intent": "Plucked string sound",
            "musicxml": "<technical><pizzicato/></technical>"
        },
        {
            "syntax": "!arco!",
            "category": "technique",
            "function": "set_technique_arco",
            "musical_intent": "Return to bowed playing",
            "musicxml": "<technical><arco/></technical>"
        },
        {
            "syntax": "!mute!",
            "category": "technique",
            "function": "set_technique_mute",
            "musical_intent": "Muted, softened timbre",
            "musicxml": "<technical><mute/></technical>"
        },
        {
            "syntax": "!harmonic!",
            "category": "technique",
            "function": "set_technique_harmonic",
            "musical_intent": "Flute-like overtone sound",
            "musicxml": "<technical><harmonic/></technical>"
        },
        {
            "syntax": "!p!",
            "category": "dynamic",
            "function": "set_dynamic_piano",
            "musical_intent": "Soft volume",
            "musicxml": "<dynamics><p/></dynamics>"
        },
        {
            "syntax": "!mf!",
            "category": "dynamic",
            "function": "set_dynamic_mezzo_forte",
            "musical_intent": "Moderately loud",
            "musicxml": "<dynamics><mf/></dynamics>"
        },
        {
            "syntax": "!f!",
            "category": "dynamic",
            "function": "set_dynamic_forte",
            "musical_intent": "Loud volume",
            "musicxml": "<dynamics><f/></dynamics>"
        },
        {
            "syntax": "!ff!",
            "category": "dynamic",
            "function": "set_dynamic_fortissimo",
            "musical_intent": "Very loud volume",
            "musicxml": "<dynamics><ff/></dynamics>"
        },
        {
            "syntax": "!subito-p!",
            "category": "dynamic",
            "function": "sudden_dynamic_drop",
            "musical_intent": "Immediate dramatic softening",
            "musicxml": "<dynamics><p/></dynamics><words>subito</words>"
        },
        {
            "syntax": "!cresc(start)!",
            "category": "dynamic_shape",
            "function": "start_crescendo",
            "musical_intent": "Gradually increase volume",
            "musicxml": "<wedge type=\"crescendo\"/>"
        },
        {
            "syntax": "!cresc(end)!",
            "category": "dynamic_shape",
            "function": "end_crescendo",
            "musical_intent": "End volume increase",
            "musicxml": "<wedge type=\"stop\"/>"
        },
        {
            "syntax": "(1slur-start",
            "category": "phrasing",
            "function": "start_slur",
            "musical_intent": "Begin legato phrase",
            "musicxml": "<slur number=\"1\" type=\"start\"/>"
        },
        {
            "syntax": "(1slur-end",
            "category": "phrasing",
            "function": "end_slur",
            "musical_intent": "End legato phrase",
            "musicxml": "<slur number=\"1\" type=\"stop\"/>"
        },
        {
            "syntax": "%%beam begin",
            "category": "rhythm_layout",
            "function": "force_beam_start",
            "musical_intent": "Explicit rhythmic grouping",
            "musicxml": "<beam>begin</beam>"
        },
        {
            "syntax": "%%beam end",
            "category": "rhythm_layout",
            "function": "force_beam_end",
            "musical_intent": "End rhythmic grouping",
            "musicxml": "<beam>end</beam>"
        },
        {
            "syntax": "(3:2 tuplet-start",
            "category": "rhythm",
            "function": "start_tuplet",
            "musical_intent": "Three notes in time of two",
            "musicxml": "<tuplet type=\"start\"/>"
        },
        {
            "syntax": "tuplet-end",
            "category": "rhythm",
            "function": "end_tuplet",
            "musical_intent": "End irregular rhythm group",
            "musicxml": "<tuplet type=\"stop\"/>"
        },
        {
            "syntax": "%%system-break",
            "category": "layout",
            "function": "new_system",
            "musical_intent": "Visual phrase separation",
            "musicxml": "<print new-system=\"yes\"/>"
        },
        {
            "syntax": "%%page-break",
            "category": "layout",
            "function": "new_page",
            "musical_intent": "Page turn or section break",
            "musicxml": "<print new-page=\"yes\"/>"
        },
        {
            "syntax": "%%instrument V1 violin",
            "category": "instrument",
            "function": "assign_instrument",
            "musical_intent": "Define timbre and range",
            "musicxml": "<score-instrument><instrument-name>violin</instrument-name></score-instrument>"
        },
        {
            "syntax": "%%midi-program V1 41",
            "category": "playback",
            "function": "assign_midi_program",
            "musical_intent": "Playback sound selection",
            "musicxml": "<midi-program>41</midi-program>"
        },
        {
            "syntax": "%%sound V1 strings.violin",
            "category": "playback",
            "function": "assign_sound_id",
            "musical_intent": "High-level timbre identity",
            "musicxml": "<instrument-sound>strings.violin</instrument-sound>"
        },
        {
            "syntax": "_melisma-start",
            "category": "lyrics",
            "function": "start_melisma",
            "musical_intent": "One syllable over multiple notes",
            "musicxml": "<extend type=\"start\"/>"
        },
        {
            "syntax": "_melisma-end",
            "category": "lyrics",
            "function": "end_melisma",
            "musical_intent": "End sustained syllable",
            "musicxml": "<extend type=\"stop\"/>"
        }
    ]
}